enerated":{"line":89,"column":4},"source":"components/ConductTransaction.js","original":{"line":51,"column":20}},{"generated":{"line":89,"column":11},"source":"components/ConductTransaction.js","original":{"line":51,"column":27}},{"name":"conductTransaction","generated":{"line":89,"column":13},"source":"components/ConductTransaction.js","original":{"line":51,"column":29}},{"generated":{"line":90,"column":0},"source":"components/ConductTransaction.js","original":{"line":49,"column":16}},{"generated":{"line":90,"column":20},"source":"components/ConductTransaction.js","original":{"line":48,"column":12}},{"generated":{"line":90,"column":21},"source":"components/ConductTransaction.js","original":{"line":30,"column":8}},{"generated":{"line":90,"column":22},"source":"components/ConductTransaction.js","original":{"line":29,"column":4}},{"generated":{"line":91,"column":0},"source":"components/ConductTransaction.js","original":{"line":58,"column":1}},{"generated":{"line":91,"column":1},"source":"components/ConductTransaction.js","original":{"line":6,"column":0}},{"name":"ConductTransaction","generated":{"line":93,"column":15},"source":"components/ConductTransaction.js","original":{"line":60,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, {useState}from \"react\";\nimport {FormGroup, FormControl, Button} from \"react-bootstrap\";\nimport {Link} from \"react-router-dom\";\nimport history from \"../history\";\n\nconst ConductTransaction = () => {\n    const [recipient, setRecipient] = useState(\"\");\n    const [amount, setAmount] = useState(0);\n\n    const handleRecipient =(e) =>{\n        setRecipient(e.target.value);\n    }\n    const handleAmount =(e) =>{\n        setAmount(Number(e.target.value));\n        console.log(recipient, amount);\n    }\n\n    const conductTransaction = ()=>{\n        fetch(`${document.location.origin}/api/transact`, {\n            method: \"POST\",\n            headers: {\"Content-Type\": \"application/json\"},\n            body: JSON.stringify({recipient, amount})\n        }).then(response => response.json())\n          .then(json =>{\n            alert(json.message || json.type);\n            history.push(\"/transaction-pool\");\n        });\n    }    \n    return (\n        <div className=\"ConductTransaction\">\n            <Link to=\"/\">ホーム</Link>\n            <h3>送金する</h3>\n\n            <FormGroup>\n                <FormControl\n                    input=\"text\"\n                    placeholder=\"送金先\"\n                    value={recipient}\n                    onChange={handleRecipient}\n                />\n                <FormControl\n                    input=\"number\"\n                    placeholder=\"送金量\"\n                    value={amount}\n                    onChange={handleAmount}\n                />\n            </FormGroup>\n            <div>\n                <Button\n                    bsStyle=\"danger\"\n                    onClick={conductTransaction}\n                >送信</Button>\n            </div>\n\n\n        </div>\n    )\n}\n\nexport default ConductTransaction"},"lineCount":null}},"error":null,"hash":"a9c9ca532e5ca6690729d522503a07cc","cacheData":{"env":{}}}